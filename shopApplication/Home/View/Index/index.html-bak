	<script>
		(function($){
		    $.fn.hoverDelay = function(options){
		        var defaults = {
		            hoverDuring: 200,
		            outDuring: 200,
		            hoverEvent: function(){
		                $.noop();
		            },
		            outEvent: function(){
		                $.noop();    
		            }
		        };
		        var sets = $.extend(defaults,options || {});
		        var hoverTimer, outTimer;
		        return $(this).each(function(){
		            $(this).hover(function(){
		                clearTimeout(outTimer);
		                hoverTimer = setTimeout(sets.hoverEvent, sets.hoverDuring);
		            },function(){
		                clearTimeout(hoverTimer);
		                outTimer = setTimeout(sets.outEvent, sets.outDuring);
		            });    
		        });
		    }      
		})(jQuery);

		$(function(){
			var inTime = 0;	//hover in时停留的时间
		    $('.bells').addClass('bells-down');
		    setTimeout(function(){
		    	$('.bells').addClass('bells-up');
		    },4000);
		    setTimeout(function(){
		    	$('.hello2015').addClass('down');
		    	$('.happy-new-year').hoverDelay({
		    		hoverDuring: 200,
		    		outDuring: 200,
		    		hoverEvent: function(){
		    			inTime = 0;
		    			setInterval(function(){
		    				inTime += 500;
		    			},500)
						$('.hello2015').removeClass('down').addClass('raise');
						$('.bells').removeClass('bells-up').addClass('bells-down');
		    		},
		    		outEvent: function(){
		    			if(inTime > 3000){
							$('.hello2015').addClass('down').removeClass('raise');
							$('.bells').removeClass('bells-down').addClass('bells-up');
						}else{
							setTimeout(function(){
								$('.hello2015').addClass('down').removeClass('raise');
								$('.bells').removeClass('bells-down').addClass('bells-up');	
							},3000 - inTime)
						}
		    		}
		    	})
		    },4500)
		})
	</script>
